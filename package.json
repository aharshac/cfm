{
  "name": "collaborizm-flavoured-markdown",
  "version": "1.0.1",
  "description": "Collaborizm Flavoured Markdown, i.e, Markdown used on Collaborizm.com",
  "main": "./lib/index.js",
  "scripts": {
    "build": "./node_modules/.bin/babel src -d lib",
    "postbuild": "./node_modules/.bin/uglifycss --output ./css/style.css ./src_css/style.css",
    "min": "./node_modules/.bin/babel src -d lib --presets=babili",
    "postmin": "./node_modules/.bin/uglifycss --output ./css/style.css ./src_css/style.css",
    "test": "./node_modules/.bin/mocha --compilers js:babel-core/register --reporter spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aharshac/cfm.git"
  },
  "keywords": [
    "markdown",
    "collaborizm",
    "nodejs"
  ],
  "author": "Harsha Alva",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aharshac/cfm/issues"
  },
  "homepage": "https://github.com/aharshac/cfm#readme",
  "dependencies": {
    "markdown-it": "^8.3.1",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-task-lists": "^2.0.1",
    "prismjs": "^1.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babili": "^0.1.3",
    "eslint": "^4.0.0",
    "power-assert": "^1.4.4",
    "uglifycss": "0.0.27"
  }
}
